#!/usr/local/bin/anch -run
#include <stdio.h>

int main  int argc, char** argv
    FILE *f
    char buf[2048]
    argc--
    setlocale  LC_CTYPE, ""
    if  strcmp(nl_langinfo(CODESET), "UTF-8")
        fprintf  stderr, "warning: utf-8 filenames not available\n"
    if  f=fopen(argv[argc],"r")
        if  fread(buf,1,2048,f)
            printf  ">%s",buf
        fclose  f
    return 0
